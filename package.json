{
    "name": "jdkula",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"yarn:next dev\" \"yarn:watch-resume\"",
        "watch-resume": "nodemon --watch lib/resume.yaml --watch ./generate_resume.mjs -e mjs,yaml,yml ./generate_resume.mjs",
        "build": "yarn build-resume && next build",
        "build-resume": "node ./generate_resume.mjs",
        "start": "next start",
        "lint": "eslint --ext ts,tsx,js,jsx,md,mdx --no-error-on-unmatched-pattern components lib pages",
        "export": "yarn build && next export"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.2.0",
        "@mdx-js/loader": "^2.1.3",
        "@mdx-js/react": "^2.1.3",
        "@next/mdx": "^12.2.5",
        "@react-spring/web": "^9.5.2",
        "@use-gesture/react": "^10.2.19",
        "eslint-plugin-prettier": "^4.2.1",
        "jsonresume-theme-caffeine": "^1.2.3",
        "jsonresume-theme-flat": "^0.3.7",
        "jsonresume-theme-short": "^0.1.7",
        "jsonresume-theme-stackoverflow": "^2.0.0",
        "next": "12.2.5",
        "node-fetch": "^3.2.10",
        "puppeteer": "^17.1.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-scroll": "^1.8.7",
        "react-toastify": "^9.0.8",
        "resume-schema": "^1.0.0",
        "resumed": "^1.0.3",
        "tslog": "^3.3.4"
    },
    "devDependencies": {
        "@types/js-yaml": "^4.0.5",
        "@types/node": "18.7.14",
        "@types/react": "18.0.18",
        "@types/react-dom": "18.0.6",
        "@types/react-scroll": "^1.8.4",
        "concurrently": "^7.3.0",
        "eslint": "8.23.0",
        "eslint-config-next": "12.2.5",
        "eslint-plugin-mdx": "^2.0.3",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "sass": "^1.54.8",
        "typescript": "4.8.2",
        "yaml": "^2.1.1",
        "yaml-loader": "^0.8.0"
    }
}
